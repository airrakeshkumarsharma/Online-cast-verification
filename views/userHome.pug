  #particles-js
  body#body.login(onload='ChangeCaptcha();')
    #preloader
      .book
        .book__page
        .book__page
        .book__page
    //
      Header start
      ====================
    #navigation.navbar-default.navbar-fixed-top(style='background: #046')
      .container
        // Brand and toggle get grouped for better mobile display
        .navbar-header
          button.navbar-toggle.collapsed(type='button', data-toggle='collapse', data-target='#navbar')
            span.sr-only Toggle navigation
            span.icon-bar
            span.icon-bar
            span.icon-bar
          a.navbar-brand(href='#')
            img.logo-1.img-thumbnail(src='https://www.digitalgujarat.gov.in/images/logo.png', alt='LOGO')
            img.logo-2.img-thumbnail(src='https://www.digitalgujarat.gov.in/images/logo.png', alt='LOGO')
        // Collect the nav links, forms, and other content for toggling
        nav#navbar.collapse.navbar-collapse
          ul#top-nav.nav.navbar-nav.navbar-right
            li.current
              a(href='#body') Home
            li
              a(href='#contact') Contact
            li.dropdown
              a.dropdown-toggle(data-toggle='dropdown', href='#')
                span.profile-ava
                  img(alt='', src='img/admin-icon.png', width='30px', height='30px')
                span.username #{user.user}
                b.caret
              ul.dropdown-menu.extended.logout
                .log-arrow-up
                li
                  a(href='/logout')
                    i.icon_key_alt
                    |  Log Out
        // /.navbar-collapse
      // /.container-fluid
    // Login forms started here
    .container.section
      .container-fluid
        .col-md-4
        .col-md-4
          .container
            .row
              .col-md-3.col-sm-6
                .service-box
                  .service-icon.relative
                    .front-content
                      img(src='https://www.digitalgujarat.gov.in/images/ration.png')
                      h3 Your Cast certificate
                  .service-content
                    h3 Your Certificate
                    p
                      i.fa.fa-edit(aria-hidden='true')
                    p
                      a(href='updateCast') Click Here
                    br
                    p #{user.user}
        .col-md-4
        .container.section
        .container-fluid
        .col-md-4
        .col-md-4
          .container
            .row
              .col-md-3.col-sm-6
