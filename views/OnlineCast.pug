    script.
      $("#Next_page").click(function(){
          $("#frmUploader").css("display","block");
          $("#form1").css("display","none");
      });
      $("#Previous_page").click(function(){
          $("#frmUploader").css("display","none");
          $("#form1").css("display","block");
      });