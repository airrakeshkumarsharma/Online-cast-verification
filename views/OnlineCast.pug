    //
      Header start
      ====================
    #navigation.navbar-default.navbar-fixed-top(style='background: #046')
      .container
        // Brand and toggle get grouped for better mobile display
        .navbar-header
          button.navbar-toggle.collapsed(type='button', data-toggle='collapse', data-target='#navbar')
            span.sr-only Toggle navigation
            span.icon-bar
            span.icon-bar
            span.icon-bar
          a.navbar-brand(href='#')
            img.logo-1.img-thumbnail(src='https://www.digitalgujarat.gov.in/images/logo.png', alt='LOGO')
            img.logo-2.img-thumbnail(src='https://www.digitalgujarat.gov.in/images/logo.png', alt='LOGO')
        // Collect the nav links, forms, and other content for toggling
        nav#navbar.collapse.navbar-collapse
          ul#top-nav.nav.navbar-nav.navbar-right
            li.current
              a(href='#body') Home
            li
              a(href='#contact') Contact
            li.dropdown
              a.dropdown-toggle(data-toggle='dropdown', href='#')
                span.profile-ava
                  img(alt='', src='img/admin-icon.png', width='30px', height='30px')
                span.username #{user.user}
                b.caret
              ul.dropdown-menu.extended.logout
                .log-arrow-up
                li
                  a(href='/logout')
                    i.icon_key_alt
                    |  Log Out
        // /.navbar-collapse
      // /.container-fluid
    // Login forms started here
    .container.section
      .container-fluid
        .col-md-4
        .col-md-4
          br
          br
          .jumbotron
            center
              img(src='https://www.digitalgujarat.gov.in/images/logo.png', alt='logo')
            br
            br
            b.text-center
              h2 Update Caste Certificate
            br
            form#form1(action='OnlineCast', method='POST', enctype='multipart/form-data')
              .input-group
                span.input-group-addon
                  img(src='https://image.winudf.com/v2/image/Y29tLmFkaGFycHJpbnRfaWNvbl8wX2U2ODM4Mzcz/icon.png?w=170&fakeurl=1&type=.png', width='10px', height='10px')
                input#text.form-control(type='text', title='Enter Adhaar number', name='adhaar', placeholder='Adhaar')
              br
              .input-group
                span.input-group-addon
                  i.fa.fa-user
                input#text.form-control(type='text', name='first_name', placeholder='first name', required)
              br
              .input-group
                span.input-group-addon
                  i.fa.fa-user
                input#text.form-control(type='text', name='last_name', placeholder='last name', required)
              br
              .input-group
                span.input-group-addon
                  i.fa.fa-user
                input#text.form-control(type='date', name='dob', placeholder='date of birth', required)
              br
              .input-group
                span.input-group-addon
                  i.fa.fa-user
                input#text.form-control(type='email', name='email', placeholder='email', required)
              br
              .input-group
                span.input-group-addon
                  i.fa.fa-user
                input#text.form-control(type='text', name='mobile', placeholder='mobile number', required)
              br
              .input-group
                span.input-group-addon
                  i.fa.fa-address-card-o
                input#text.form-control(type='text', name='address', placeholder='address', required)
              br
              .input-group
                span.input-group-addon
                  i.fa.fa-map
                input#text.form-control(type='text', name='city', placeholder='city', required)
              br
              .input-group
                span.input-group-addon
                  i.fa.fa-list
                select#state.form-control.selectpicker(name='state')
                  option(value='') Please select your state
                  option Gujrat
                  option Assam
                  option Bihar
                  option AP
                  option UP
              br 
              .input-group
                span.input-group-addon
                  i.fa.fa-image
                input#file.form-control(type='text', name='zip', title='zip_code',  placeholder='zip code', required)
              br
              .input-group
                table
                  tr
                    
                    | Gender 
                    td
                      | Male 
                      input(type='radio', name='gender', value='female')
                      |     Female 
                      input(type='radio', name='gender', value='female')
              br 
              .input-group
                span.input-group-addon
                  i.fa.fa-image
                input#file.form-control(type='file', name='image', title='picture of user',  placeholder='file', required)
              br
              .form-group
                input.form-control.btn.btn-warning.block#form_sub1(type='submit', name='', onclick='upload()', value=Submit, required)
              br
              .form-group
                input.btn.btn-default.pull-right.block#Next_page(type='button', name='', onclick='upload()', value='Next',align='right')
              br
              #msg
            form#frmUploader(enctype='multipart/form-data', action='Upload', method='post',style='display:none')
              br
              b.text-center
                h2 Upload Caste Certificate
              br
              .input-group
                span.input-group-addon
                  i.fa.fa-image
                input.form-control(type='file', name='imgUploader', multiple)
              br
              .form-group
                input.form-control.btn.btn-warning(type='submit', name="submit", id='btnSubmit', value='Upload')
              br
              .form-group
                input.btn.btn-default.pull-left.block#Previous_page(type='button', name='', onclick='upload()', value='Previous',align='right')
    // Login forms Ended here
