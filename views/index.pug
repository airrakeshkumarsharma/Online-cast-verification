doctype html
html.no-js
  head
    meta(charset='utf-8')
    meta(http-equiv='X-UA-Compatible', content='IE=edge')
    title Gujrat
    meta(name='viewport', content='width=device-width, initial-scale=1')
    // Place favicon.ico and apple-touch-icon.png in the root directory
    // Fonts
    // Lato
    link(href='http://fonts.googleapis.com/css?family=Lato:400,300,700', rel='stylesheet', type='text/css')
    link(rel='stylesheet', href='http://code.ionicframework.com/ionicons/2.0.1/css/ionicons.min.css')
    link(href='https://maxcdn.bootstrapcdn.com/font-awesome/4.7.0/css/font-awesome.min.css', rel='stylesheet', integrity='sha384-wvfXpqpZZVQGK6TAh5PVlGOfQNHSoD2xbE+QkPxCAFlNEevoEH3Sl0sibVcOQVnN', crossorigin='anonymous')
    // CSS
    link(rel='stylesheet', href='css/bootstrap.min.css')
    link(rel='stylesheet', href='css/font-awesome.min.css')
    link(rel='stylesheet', href='css/owl.carousel.css')
    link(rel='stylesheet', href='css/animate.css')
    link(rel='stylesheet', href='css/main.css')
    // Responsive Stylesheet
    link(rel='stylesheet', href='css/responsive.css')
  body#body
    #preloader
      .book
        .book__page
        .book__page
        .book__page
    //
      Header start
      ====================
    #navigation.navbar-default.navbar-fixed-top(style='background: #046')
      .container
        // Brand and toggle get grouped for better mobile display
        .navbar-header
          button.navbar-toggle.collapsed(type='button', data-toggle='collapse', data-target='#navbar')
            span.sr-only Toggle navigation
            span.icon-bar
            span.icon-bar
            span.icon-bar
          a.navbar-brand(href='#')
            img.logo-1.img-thumbnail(src='https://www.digitalgujarat.gov.in/images/logo.png', alt='LOGO')
            img.logo-2.img-thumbnail(src='https://www.digitalgujarat.gov.in/images/logo.png', alt='LOGO')
        // Collect the nav links, forms, and other content for toggling
        nav#navbar.collapse.navbar-collapse
          ul#top-nav.nav.navbar-nav.navbar-right
            li.current
              a(href='#') Home
            li
              a(href='about')
                i.fa.fa-sliders-h
                |  About us
            li
              a(href='services') Services
            li
              a(href='login') Login
            li
              a(href='contact') Contact
        // /.navbar-collapse
      // /.container-fluid
    section(clas='wow fadeInUp')
      .container.section
        .map-wrapper
          .container-fluid
            .col-md-12
              #myCarousel.carousel.slide(data-ride='carousel')
                ol.carousel-indicators
                  li.active(data-target='#myCarousel', data-slide-to='0')
                  li(data-target='#myCarousel', data-slide-to='1')
                .carousel-inner
                  .item.active
                    img.img-thumbnail(src='images/s1.jpg', style='width: 100%; height: 100%', alt='minister')
                  .item
                    img.img-thumbnail(src='images/s2.jpg', style='width: 100%;height: 100%', alt='HACKATHON')
    footer
      .container
        .row
          .col-md-12
            .block
              p
                | Copyright Â© 
                a(href='http://www.i4c.in') SMART INDIA HACKATHON
                | | All right reserved.
    // Js
    script(src='js/vendor/modernizr-2.6.2.min.js')
    script(src='js/vendor/jquery-1.10.2.min.js')
    script(src='js/jquery.lwtCountdown-1.0.js')
    script(src='js/bootstrap.min.js')
    script(src='js/owl.carousel.min.js')
    script(src='js/jquery.validate.min.js')
    script(src='js/jquery.form.js')
    script(src='js/jquery.nav.js')
    script(src='js/jquery.sticky.js')
    script(src='js/plugins.js')
    script(src='js/wow.min.js')
    script(src='js/main.js')
